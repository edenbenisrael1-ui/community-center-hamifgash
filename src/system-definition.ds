											conflict_activity = "";
											all_activities = Activities[ID != 0];
											for each  check_activity in all_activities
											{
												// דלג על הפעילות הנוכחית
												if(input.ID != null && check_activity.ID == input.ID)
												{
													continue;
												}
												// בדוק רק אם באותו חדר ויום (ללא בדיקת זמנים)
												if(check_activity.lookup_room_name == input.lookup_room_name && check_activity.activity_day == input.activity_day)
												{
													conflict_found = true;
													conflict_activity = check_activity.activity_name;
													break;
												}
											}
											// אם נמצאה התנגשות
											if(conflict_found)
											{
												alert " החדר כבר בשימוש ב " + input.activity_day + " עבור החוג " + conflict_activity + "אנא בחר חדר אחר";
