						(
											disable subform_payment_history.transferred;
										disable subform_payment_history.lookup_participant_name;
										disable subform_payment_history.activity_name_payment_history;
										disable subform_payment_history.lookup_payment_method;
						)
					}
				}

			}
		}






		functions
		{
			Making_a_payment_for_clas as "Making a payment for classes"
			{
				type =  functions
				form = Payments
				execution type = for each record	
				on start
				{
					actions 
					{
					on click
					(
						// חישוב יתרה לתשלום
										if(input.balance_to_be_paid == null)
										{
											if(input.total_payment != null && input.transferred != null)
											{
												input.balance_to_be_paid = input.total_payment - input.transferred;
											}
										}
										else if(input.transferred != null)
										{
											input.balance_to_be_paid = input.balance_to_be_paid - input.transferred;
										}
										// שליחת מייל
										participate_rec = Participates[ID == input.lookup_participant_name];
										if(participate_rec != null && participate_rec.participate_email != null)
										{
											sendmail
											[
												from :zoho.adminuserid
												to :participate_rec.participate_email
												subject :"ביצוע תשלום עבור משתתף/ת: " + input.lookup_participant_name.participate_name
												message :"הי, בוצע תשלום על סך " + input.transferred + ". היתרה לתשלום: " + input.balance_to_be_paid
											]
										}
										// הוספת רשומה לטופס Payment_History
										hist_rec = insert into Payment_History
										[
											lookup_payment_method=input.ID
											lookup_participant_name=input.lookup_participant_name
											transferred=input.transferred
											Added_User=zoho.loginuser
											activity_name_payment_history=input.invoice.activity_name
										];
					)
					}
				}
			}
		}
