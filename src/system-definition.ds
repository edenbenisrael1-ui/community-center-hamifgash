v class=&quot;activity-name&quot;&gt;\u05DE\u05D3\u05E2\u05D9\u05DD&lt;\/div&gt;\n                    &lt;div class=&quot;activity-details&quot;&gt;\n                        &lt;div class=&quot;activity-room&quot;&gt;\u05DE\u05E2\u05D1\u05D3\u05D4 A&lt;\/div&gt;\n                        &lt;div class=&quot;activity-price&quot;&gt;\u20AA1,213&lt;\/div&gt;\n                    &lt;\/div&gt;\n                &lt;\/div&gt;\n            &lt;\/div&gt;\n            &lt;div class=&quot;activity-cell&quot;&gt;\n                &lt;div class=&quot;activity computers&quot;&gt;\n                    &lt;div class=&quot;activity-name&quot;&gt;\u05DE\u05D7\u05E9\u05D1\u05D9\u05DD&lt;\/div&gt;\n                    &lt;div class=&quot;activity-details&quot;&gt;\n                        &lt;div class=&quot;activity-room&quot;&gt;\u05D7\u05D3\u05E8 D&lt;\/div&gt;\n                        &lt;div class=&quot;activity-price&quot;&gt;\u20AA1,757&lt;\/div&gt;\n                    &lt;\/div&gt;\n                &lt;\/div&gt;\n            &lt;\/div&gt;\n            &lt;div class=&quot;activity-cell&quot;&gt;\n                &lt;div class=&quot;activity languages&quot;&gt;\n                    &lt;div class=&quot;activity-name&quot;&gt;\u05E8\u05D5\u05D1\u05D5\u05D8\u05D9\u05E7\u05D4&lt;\/div&gt;\n                    &lt;div class=&quot;activity-details&quot;&gt;\n                        &lt;div class=&quot;activity-room&quot;&gt;\u05D7\u05D3\u05E8 I&lt;\/div&gt;\n                        &lt;div class=&quot;activity-price&quot;&gt;\u20AA1,235&lt;\/div&gt;\n                    &lt;\/div&gt;\n                &lt;\/div&gt;\n            &lt;\/div&gt;\n            &lt;div class=&quot;activity-cell empty-cell&quot;&gt;\u05D6\u05DE\u05DF \u05E4\u05E0\u05D5\u05D9&lt;\/div&gt;\n            &lt;div class=&quot;activity-cell empty-cell&quot;&gt;\u05E9\u05D1\u05EA \u05E7\u05D5\u05D3\u05E9&lt;\/div&gt;\n\n            &lt;!-- 18:00-19:00 --&gt;\n            &lt;div class=&quot;time-slot&quot;&gt;\n                &lt;div class=&quot;time-main&quot;&gt;18:00&lt;\/div&gt;\n                &lt;div class=&quot;time-sub&quot;&gt;19:00&lt;\/div&gt;\n            &lt;\/div&gt;\n            &lt;div class=&quot;activity-cell&quot;&gt;\n                &lt;div class=&quot;activity fitness&quot;&gt;\n                    &lt;div class=&quot;activity-name&quot;&gt;\u05DB\u05D3\u05D5\u05E8\u05D2\u05DC&lt;\/div&gt;\n                    &lt;div class=&quot;activity-details&quot;&gt;\n                        &lt;div class=&quot;activity-room&quot;&gt;\u05D0\u05D5\u05DC\u05DD D&lt;\/div&gt;\n                        &lt;div class=&quot;activity-price&quot;&gt;\u20AA1,406&lt;\/div&gt;\n                    &lt;\/div&gt;\n                &lt;\/div&gt;\n            &lt;\/div&gt;\n            &lt;div class=&quot;activity-cell&quot;&gt;\n                &lt;div class=&quot;activity art&quot;&gt;\n                    &lt;div class=&quot;activity-name&quot;&gt;\u05EA\u05D9\u05D0\u05D8\u05E8\u05D5\u05DF&lt;\/div&gt;\n                    &lt;div class=&quot;activity-details&quot;&gt;\n                        &lt;div class=&quot;activity-room&quot;&gt;\u05D1\u05D9\u05DE\u05D4 A&lt;\/div&gt;\n                        &lt;div class=&quot;activity-price&quot;&gt;\u20AA1,398&lt;\/div&gt;\n                    &lt;\/div&gt;\n                &lt;\/div&gt;\n            &lt;\/div&gt;\n            &lt;div class=&quot;activity-cell&quot;&gt;\n                &lt;div class=&quot;activity dance&quot;&gt;\n                    &lt;div class=&quot;activity-name&quot;&gt;\u05D2\u05D5\u05DC\u05E3&lt;\/div&gt;\n                    &lt;div class=&quot;activity-details&quot;&gt;\n                        &lt;div class=&quot;activity-room&quot;&gt;\u05DE\u05D2\u05E8\u05E9 A&lt;\/div&gt;\n                        &lt;div class=&quot;activity-price&quot;&gt;\u20AA1,624&lt;\/div&gt;\n                    &lt;\/div&gt;\n                &lt;\/div&gt;\n            &lt;\/div&gt;\n            &lt;div class=&quot;activity-cell empty-cell&quot;&gt;\u05D6\u05DE\u05DF \u05E4\u05E0\u05D5\u05D9&lt;\/div&gt;\n            &lt;div class=&quot;activity-cell empty-cell&quot;&gt;\u05D6\u05DE\u05DF \u05E4\u05E0\u05D5\u05D9&lt;\/div&gt;\n            &lt;div class=&quot;activity-cell empty-cell&quot;&gt;\u05D6\u05DE\u05DF \u05E4\u05E0\u05D5\u05D9&lt;\/div&gt;\n            &lt;div class=&quot;activity-cell empty-cell&quot;&gt;\u05E9\u05D1\u05EA \u05E7\u05D5\u05D3\u05E9&lt;\/div&gt;\n        &lt;\/div&gt;\n\n        &lt;div class=&quot;schedule-stats&quot;&gt;\n            &lt;div class=&quot;stat-item&quot;&gt;\n                &lt;div class=&quot;stat-number&quot;&gt;27&lt;\/div&gt;\n                &lt;div class=&quot;stat-label&quot;&gt;\u05D7\u05D5\u05D2\u05D9\u05DD \u05E4\u05E2\u05D9\u05DC\u05D9\u05DD&lt;\/div&gt;\n            &lt;\/div&gt;\n            &lt;div class=&quot;stat-item&quot;&gt;\n                &lt;div class=&quot;stat-number&quot;&gt;\u20AA32,424&lt;\/div&gt;\n                &lt;div class=&quot;stat-label&quot;&gt;\u05E1\u05D4&quot;\u05DB \u05D4\u05DB\u05E0\u05E1\u05D5\u05EA \u05E9\u05D1\u05D5\u05E2\u05D9\u05D5\u05EA&lt;\/div&gt;\n            &lt;\/div&gt;\n            &lt;div class=&quot;stat-item&quot;&gt;\n                &lt;div class=&quot;stat-number&quot;&gt;15&lt;\/div&gt;\n                &lt;div class=&quot;stat-label&quot;&gt;\u05D7\u05D3\u05E8\u05D9\u05DD \u05D1\u05E9\u05D9\u05DE\u05D5\u05E9&lt;\/div&gt;\n            &lt;\/div&gt;\n            &lt;div class=&quot;stat-item&quot;&gt;\n                &lt;div class=&quot;stat-number&quot;&gt;27&lt;\/div&gt;\n                &lt;div class=&quot;stat-label&quot;&gt;\u05E9\u05E2\u05D5\u05EA \u05D4\u05D5\u05E8\u05D0\u05D4 \u05E9\u05D1\u05D5\u05E2\u05D9\u05D5\u05EA&lt;\/div&gt;\n            &lt;\/div&gt;\n            &lt;div class=&quot;stat-item&quot;&gt;\n                &lt;div class=&quot;stat-number&quot;&gt;320+&lt;\/div&gt;\n                &lt;div class=&quot;stat-label&quot;&gt;\u05DE\u05E9\u05EA\u05EA\u05E4\u05D9\u05DD \u05E8\u05E9\u05D5\u05DE\u05D9\u05DD&lt;\/div&gt;\n            &lt;\/div&gt;\n        &lt;\/div&gt;\n    &lt;\/div&gt;\n\n    &lt;script&gt;\n        \/\/ \u05E2\u05D3\u05DB\u05D5\u05DF \u05D6\u05DE\u05DF \u05E0\u05D5\u05DB\u05D7\u05D9\n        function updateDateTime() {\n            const now = new Date();\n            const options = { \n                weekday: &apos;long&apos;,\n                year: &apos;numeric&apos;, \n                month: &apos;long&apos;, \n                day: &apos;numeric&apos;,\n                hour: &apos;2-digit&apos;,\n                minute: &apos;2-digit&apos;,\n                second: &apos;2-digit&apos;\n            };\n            \n            document.getElementById(&apos;currentDateTime&apos;).textContent = \n                now.toLocaleDateString(&apos;he-IL&apos;, options);\n        }\n\n        updateDateTime();\n        setInterval(updateDateTime, 1000);\n\n        \/\/ \u05D0\u05E4\u05E7\u05D8\u05D9\u05DD \u05D0\u05D9\u05E0\u05D8\u05E8\u05D0\u05E7\u05D8\u05D9\u05D1\u05D9\u05D9\u05DD \u05DC\u05D7\u05D5\u05D2\u05D9\u05DD\n        document.querySelectorAll(&apos;.activity&apos;).forEach(activity =&gt; {\n            activity.addEventListener(&apos;mouseenter&apos;, function() {\n                this.style.transform = &apos;translateY(-2px) scale(1.05)&apos;;\n            });\n            \n            activity.addEventListener(&apos;mouseleave&apos;, function() {\n                this.style.transform = &apos;translateY(0) scale(1)&apos;;\n            });\n        });\n\n        \/\/ \u05E4\u05D5\u05E0\u05E7\u05E6\u05D9\u05D5\u05E0\u05DC\u05D9\u05D5\u05EA \u05DC\u05D7\u05D9\u05E6\u05D4 \u05E2\u05DC \u05D7\u05D5\u05D2\u05D9\u05DD\n        document.querySelectorAll(&apos;.activity&apos;).forEach(activity =&gt; {\n            activity.addEventListener(&apos;click&apos;, function() {\n                const activityName = this.querySelector(&apos;.activity-name&apos;).textContent.trim();\n                const room = this.querySelector(&apos;.activity-room&apos;).textContent.trim();\n                const price = this.querySelector(&apos;.activity-price&apos;).textContent.trim();\n                \n                alert(`? \u05E4\u05E8\u05D8\u05D9 \u05D4\u05D7\u05D5\u05D2:\\n\\n` +\n                      `? ${activityName}\\n` +\n                      `? ${room}\\n` +\n                      `? ${price}\\n\\n` +\n                      `\u05DC\u05D7\u05E5 OK \u05DC\u05D7\u05D6\u05D5\u05E8 \u05DC\u05DE\u05E2\u05E8\u05DB\u05EA \u05D4\u05E9\u05E2\u05D5\u05EA`);\n            });\n        });\n\n        \/\/ \u05D4\u05D3\u05D2\u05E9\u05EA \u05D4\u05E9\u05E2\u05D4 \u05D4\u05E0\u05D5\u05DB\u05D7\u05D9\u05EA\n        function highlightCurrentTime() {\n            const now = new Date();\n            const currentHour = now.getHours();\n            \n            \/\/ \u05D4\u05E1\u05E8\u05EA \u05D4\u05D3\u05D2\u05E9\u05D5\u05EA \u05E7\u05D5\u05D3\u05DE\u05D5\u05EA\n            document.querySelectorAll(&apos;.activity&apos;).forEach(activity =&gt; {\n                activity.style.boxShadow = activity.style.boxShadow.replace(&apos;inset&apos;, &apos;&apos;);\n            });\n            \n            \/\/ \u05D4\u05D3\u05D2\u05E9\u05EA \u05D7\u05D5\u05D2\u05D9\u05DD \u05E9\u05DE\u05EA\u05E7\u05D9\u05D9\u05DE\u05D9\u05DD \u05E2\u05DB\u05E9\u05D9\u05D5\n            const timeSlots = {\n                10: [&apos;.activity.ai&apos;],\n                12: [&apos;.activity.music&apos;, &apos;.activity.photography&apos;],\n                13: [&apos;.activity.voice&apos;, &apos;.activity.dance&apos;],\n                15: [&apos;.activity.craft&apos;, &apos;.activity.therapy&apos;],\n                16: [&apos;.activity.singing&apos;, &apos;.activity.yoga&apos;],\n                17: [&apos;.activity.zumba&apos;, &apos;.activity.computers&apos;],\n                18: [&apos;.activity.fitness&apos;, &apos;.activity.art&apos;]\n            };\n            \n            if (timeSlots[currentHour]) {\n                timeSlots[currentHour].forEach(selector =&gt; {\n                    const activity = document.querySelector(selector);\n                    if (activity) {\n                        activity.style.boxShadow += &apos;inset 0 0 20px rgba(255, 255, 255, 0.5)&apos;;\n                    }\n                });\n            }\n        }\n\n        highlightCurrentTime();\n        setInterval(highlightCurrentTime, 60000); \/\/ \u05D1\u05D3\u05D9\u05E7\u05D4 \u05DB\u05DC \u05D3\u05E7\u05D4\n    &lt;\/script&gt;\n&lt;\/body&gt;\n&lt;\/html&gt;\n&lt;%\n\n}%&gt;]]>\n<\/dsp>\n<\/column>\n<\/row>\n<\/layout>\n<\/zml>"
		}
	}


