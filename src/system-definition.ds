			Show_activities_and_price as "Show activities and price"
			{
				type =  form
				form = Payments
				record event = on edit

				on load
				{
					actions 
					{
						custom deluge script
						(
											// Workflow עבור טופס Payments
										// Event: on user input of lookup_participant_name
										participant_id = input.lookup_participant_name;
										if(participant_id != null)
										{
											// מציאת החוגים של המשתתף
											participant_activities = Partners_Activity[lookup_name_participant == participant_id && registered == true];
											if(participant_activities.count() > 0)
											{
												// ניקוי ה subform
												input.invoice.clear();
												// יצירת שורות חדשות
												invoice_rows = Collection();
												total = 0;
												for each  rec in participant_activities
												{
													// קבלת פרטי החוג
													activity = Activities[ID == rec.lookup_activity];
													if(activity != null)
													{
														// יצירת שורה חדשה
														row = Payments.invoice();
														row.activity_name=activity.activity_name;
														row.participate_price=activity.participate_price;
														// הוספה לאוסף
														invoice_rows.insert(row);
														// חישוב סכום
														total = total + activity.participate_price;
													}
												}
												// הכנסת השורות לsubform
												input.invoice.insert(invoice_rows);
												// עדכון הסכום הכולל
												input.total_payment = total;
												input.balance_to_be_paid = total - input.transferred;
												info "נוספו " + participant_activities.count() + " חוגים. סכום: " + total;
											}
											else
											{
												alert "לא נמצאו חוגים למשתתף";
												input.invoice.clear();
												input.total_payment = 0;
											}
										}
						)
					}
				}

			}
			The_subform_payment_histo1 as "The subform-payment-history will not be editable"
			{
				type =  form
				form = Payments
				record event = on edit

				on load
