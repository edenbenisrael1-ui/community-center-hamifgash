													{
														// בדיקה שהמשתתף קיים ולא ריק
														if(participant_record.lookup_name_participant != null)
														{
															// יצירת שורה חדשה בסאב פורם
															new_row = Presence.subform_attendance_by_class();
															// העתקת הנתונים מהרשומה הקיימת
															new_row.activities_class_attendance=participant_record.lookup_activity;
															new_row.Lookup=participant_record.lookup_name_participant;
															new_row.was_present=false;
															// סימון נוכחות - ברירת מחדל false
															attendance_rows.insert(new_row);
														}
													}
													// הכנסת כל השורות לטופס
													input.subform_attendance_by_class.insert(attendance_rows);
												}
												else
												{
													alert "החוג שנבחר לא כולל משתתפים פעילים";
												}
											}
										}
										else
										{
											alert "אנא בחר גם חוג וגם תאריך לפני מילוי רשימת הנוכחות";
										}
						)
					}
				}

			}
